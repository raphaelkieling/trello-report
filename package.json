{
  "name": "trello-report-js",
  "version": "1.0.3",
  "engines": {
    "node": ">=5.0.0"
  },
  "main": "dist/trello_report.min.js",
  "repository": "https://github.com/raphaelkieling/trello-report",
  "author": "Raphael Kieling <raphael_kieling@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node server/server",
    "build": "npm run test && ./node_modules/.bin/gulp",
    "test": "./node_modules/.bin/mocha",
    "test:coverage":
      "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "coveralls": "./node_modules/.bin/coveralls",
    "precommit": "yarn test &&pretty-quick --staged",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "colors": "1.2.3",
    "express": "4.16.3",
    "node-trello": "1.3.0",
    "paretojs": "3.7.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.1",
    "nyc": "11.7.1",
    "prettier": "1.12.1",
    "pretty-quick": "1.4.1",
    "mocha": "5.1.1",
    "gulp": "^3.9.1",
    "gulp-minify": "^2.1.0",
    "husky": "0.14.3",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
